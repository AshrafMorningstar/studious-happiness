{
	"name": "ashrafmorningstar-professional-showcase",
	"version": "1.0.0",
	"description": "Professional GitHub Profile Visuals by AshrafMorningstar",
	"author": "AshrafMorningstar <https://github.com/AshrafMorningstar>",
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"start": "remotion studio --props ./input.json",
		"render": "npm run render:stats && npm run render:languages && npm run render:readme && npm run render:main-stats && npm run render:issue-tracking && npm run render:code-metrics && npm run render:activity-overview && npm run render:commit-streak && npm run render:top-languages && npm run render:dashboard",
		"render:stats": "npx remotion render --props ./input.json stats out/stats.gif",
		"render:languages": "npx remotion render --props ./input.json languages out/languages.gif",
		"render:readme": "npx remotion render --props ./input.json readme out/readme.gif",
		"render:main-stats": "npx remotion render --props ./input.json main-stats out/main-stats.gif",
		"render:issue-tracking": "npx remotion render --props ./input.json issue-tracking out/issue-tracking.gif",
		"render:code-metrics": "npx remotion render --props ./input.json code-metrics out/code-metrics.gif",
		"render:activity-overview": "npx remotion render --props ./input.json activity-overview out/activity-overview.gif",
		"render:commit-streak": "npx remotion render --props ./input.json commit-streak out/commit-streak.gif",
		"render:top-languages": "npx remotion render --props ./input.json top-languages out/top-languages.gif",
		"render:dashboard": "npx remotion render --props ./input.json dashboard out/dashboard.gif",
		"upgrade": "remotion upgrade",
		"test": "eslint src --ext ts,tsx,js,jsx && tsc"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AshrafMorningstar/supreme-guacamole.git"
	},
	"license": "MIT",
	"dependencies": {
		"@remotion/bundler": "^4.0.214",
		"@remotion/cli": "^4.0.214",
		"@remotion/zod-types": "^4.0.214",
		"axios": "^1.7.7",
		"clsx": "^2.1.1",
		"framer-motion": "^11.9.0",
		"lucide-react": "^0.446.0",
		"octokit": "^4.0.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"remotion": "^4.0.214",
		"tailwind-merge": "^2.5.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@remotion/eslint-config": "^4.0.214",
		"@remotion/tailwind": "^4.0.214",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^19.2.3",
		"@types/web": "^0.0.167",
		"eslint": "^9.11.1",
		"prettier": "^3.3.3",
		"typescript": "^5.6.2"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"webpack"
			]
		}
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
